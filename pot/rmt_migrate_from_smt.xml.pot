#. Tag: title
#: rmt_migrate_from_smt.xml:552
#, no-c-format
msgid "Exporting &smt; Data"
msgstr ""

#. Tag: title
#: rmt_migrate_from_smt.xml:554
#, no-c-format
msgid "Export &smt; Data"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:556
#, no-c-format
msgid "Update your SMT server installation by running <command>zypper up</command> ."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:562
#, no-c-format
msgid "If you want to export your SSL certificates along with the rest of the data, run <command>smt-data-export</command> . Remember to keep your certificates in a safe place."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:567
#, no-c-format
msgid "If you do not want to export the SSL certificates from &smt; run <command>smt-data-export --no-ssl-export</command> ."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:573
#, no-c-format
msgid "The exported configuration is now saved to <filename>smt-export.<replaceable>XXXXXX</replaceable>.tar.gz</filename> . Copy the file to a location which can be accessed by the new &rmt; server."
msgstr ""

#. Tag: title
#: rmt_migrate_from_smt.xml:584
#, no-c-format
msgid "Importing &smt; Data to &rmt;"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:586
#, no-c-format
msgid "To make sure your &rmt; installation is up-to-date, run <command>zypper up</command> ."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:592
#, no-c-format
msgid "Copy the exported <literal>.tar.gz</literal> file to an empty directory, then unpack it:"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:596
#, no-c-format
msgid "&prompt.user;<command>mkdir <replaceable>EMPTY_DIR</replaceable></command>\n"
      "&prompt.user;<command>cd <replaceable>EMPTY_DIR</replaceable></command>\n"
      "&prompt.user;<command>cp <replaceable>/PATH/TO/</replaceable>smt-export.<replaceable>XXXXXX</replaceable>.tar.gz ./</command>\n"
      "&prompt.user;<command>tar xf smt-export.<replaceable>XXXXXX</replaceable>.tar.gz</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:602
#, no-c-format
msgid "If you chose to export the SSL certificates from &smt;, copy the CA private key and certificate to <filename>/usr/share/rmt/ssl/</filename> :"
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:607
#, no-c-format
msgid "&prompt.sudo;<command>cp ssl/cacert.key /usr/share/rmt/ssl/rmt-ca.key</command>\n"
      "&prompt.sudo;<command>cp ssl/cacert.pem /usr/share/rmt/ssl/rmt-ca.crt</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:611
#, no-c-format
msgid "Run the &yast; &rmt; configuration module as described in <xref linkend=\"sec.rmt_installation.yast_configuration\"/> . If you imported the &smt; CA certificate, add the domain of the &smt; server to the common names of the new SSL certificate."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:619
#, no-c-format
msgid "Run the &rmt; synchronization to get the products and repositories data from &scc;."
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:623
#, no-c-format
msgid "&prompt.sudo;<command>rmt-cli sync</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:626
#, no-c-format
msgid "Import the data from the &smt; server."
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:629
#, no-c-format
msgid "&prompt.sudo;<command>rmt-data-import -d ./</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:632
#, no-c-format
msgid "Optional: If the URL of the &rmt; server changed, change the URL parameter of clients in the /etc/SUSEConnect to point to the new &rmt; server. Alternatively, change the DNS records to the re-assign the host name to the &rmt; server."
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:640
#, no-c-format
msgid "Optional: Move the mirrored repository data from to &rmt; and adjust the ownership of the copied data."
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:644
#, no-c-format
msgid "&prompt.sudo;<command>cp -r /var/www/htdocs/repo/* /var/lib/rmt/public/repo</command>\n"
      "&prompt.sudo;<command>chown -R _rmt:nginx /var/lib/rmt/public/repo</command>"
msgstr ""

#. Tag: para
#: rmt_migrate_from_smt.xml:648
#, no-c-format
msgid "Update the packages in the repositories."
msgstr ""

#. Tag: screen
#: rmt_migrate_from_smt.xml:651
#, no-c-format
msgid "&prompt.sudo;<command>rmt-cli mirror</command>"
msgstr ""

