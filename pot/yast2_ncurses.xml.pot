#. Tag: term
#: yast2_ncurses.xml:619
#, no-c-format
msgid "dnsrecord"
msgstr ""

#. Tag: para
#: yast2_ncurses.xml:621
#, no-c-format
msgid "Configures zone resource records:"
msgstr ""

#. Tag: screen
#: yast2_ncurses.xml:624
