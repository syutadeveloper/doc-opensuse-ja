msgstr ""

#. Tag: para
#: tuning_utilities_dstat.xml:61
#, no-c-format
msgid "expand <option>-C</option> , <option>-D</option> , <option>-I</option> , <option>-N</option> and <option>-S</option> discovery lists"
msgstr ""

#. Tag: term
#: tuning_utilities_dstat.xml:69
#, no-c-format
msgid "<option>-v</option> , <option>--vmstat</option>"
msgstr ""

#. Tag: para
#: tuning_utilities_dstat.xml:71
#, no-c-format
