#: tuning_utilities_dstat.xml:51
#, no-c-format
msgid "<option>-a</option> , <option>--all</option>"
msgstr ""

#. Tag: para
#: tuning_utilities_dstat.xml:53
#, no-c-format
msgid "equal to <option>-cdngy</option> (default)"
msgstr ""

