#: yast2_ncurses.xml:1300
#, no-c-format
msgid ""
"\n"
"&prompt.sudo;yast kdump immediatereboot enable\n"
"&prompt.sudo;yast kdump immediatereboot disable\n"
msgstr ""
"\n"
"&prompt.sudo;yast kdump immediatereboot enable\n"
"&prompt.sudo;yast kdump immediatereboot disable\n"

#. Tag: term
#: yast2_ncurses.xml:1307
#, no-c-format
msgid "keepolddumps"
msgstr "keepolddumps"

#. Tag: para
#: yast2_ncurses.xml:1309
#, no-c-format
msgid "Specifies how many old dump images are kept. Specify zero to keep them all:"
msgstr "古いダンプイメージをどこまで保存するのかを設定します。 0 を指定すると、全てを保存します:"

