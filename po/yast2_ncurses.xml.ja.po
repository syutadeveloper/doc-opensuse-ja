#: yast2_ncurses.xml:1981
#, no-c-format
msgid ""
"\n"
"&prompt.sudo;yast samba-client joindomain domain=SMB_DOMAIN user=username "
"password=pwd\n"
msgstr ""
"\n"
"&prompt.sudo;yast samba-client joindomain domain=SMB_DOMAIN user=username "
"password=pwd\n"

#. Tag: term
#: yast2_ncurses.xml:1987
#, no-c-format
msgid "winbind"
msgstr "winbind"

