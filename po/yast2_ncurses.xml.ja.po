msgstr ""
"\n"
"&prompt.sudo;yast kdump notificationcc email=\"user1@example.com "
"user2@example.com\"\n"

#. Tag: term
#: yast2_ncurses.xml:1354
#, no-c-format
msgid "notificationto"
msgstr "notificationto"

#. Tag: para
#: yast2_ncurses.xml:1356
#, no-c-format
msgid "Specifies an e-mail address for sending notification messages:"
msgstr "通知メッセージの送信先電子メールアドレスを設定します:"

#. Tag: screen
#: yast2_ncurses.xml:1359
#, no-c-format
msgid ""
"\n"
"&prompt.sudo;yast kdump notificationto email=\"user1@example.com "
"user2@example.com\"\n"
msgstr ""
"\n"
"&prompt.sudo;yast kdump notificationto email=\"user1@example.com "
"user2@example.com\"\n"

#. Tag: para
#: yast2_ncurses.xml:1367
#, no-c-format
msgid "Displays <systemitem>kdump</systemitem> settings:"
msgstr "<systemitem>kdump</systemitem> の設定を表示します:"

