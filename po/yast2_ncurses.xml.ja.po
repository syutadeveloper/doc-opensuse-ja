"&prompt.sudo;yast kdump startup disable\n"
msgstr ""
"\n"
"&prompt.sudo;yast kdump startup enable alloc_mem=128,256\n"
"&prompt.sudo;yast kdump startup disable\n"

#. Tag: title
#: yast2_ncurses.xml:1433
#, no-c-format
msgid "yast keyboard"
msgstr "yast keyboard"

#. Tag: para
#: yast2_ncurses.xml:1434
#, no-c-format
msgid ""
