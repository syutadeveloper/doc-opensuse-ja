#. Tag: screen
#: yast2_ncurses.xml:1337
#, no-c-format
msgid ""
"\n"
"&prompt.sudo;yast kdump kernelcommandlineappend command=\"ro root=LABEL=/\"\n"
msgstr ""
"\n"
"&prompt.sudo;yast kdump kernelcommandlineappend command=\"ro root=LABEL=/\"\n"

#. Tag: term
#: yast2_ncurses.xml:1343
#, no-c-format
msgid "notificationcc"
msgstr "notificationcc"

