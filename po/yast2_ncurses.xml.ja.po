#. Tag: screen
#: yast2_ncurses.xml:2340
#, no-c-format
msgid ""
"\n"
"&prompt.sudo;yast sysconfig list all\n"
"AOU_AUTO_AGREE_WITH_LICENSES=\"false\"\n"
"AOU_ENABLE_CRONJOB=\"true\"\n"
"AOU_INCLUDE_RECOMMENDS=\"false\"\n"
"[...]\n"
msgstr ""
"\n"
"&prompt.sudo;yast sysconfig list all\n"
"AOU_AUTO_AGREE_WITH_LICENSES=\"false\"\n"
"AOU_ENABLE_CRONJOB=\"true\"\n"
"AOU_INCLUDE_RECOMMENDS=\"false\"\n"
"[...]\n"

#. Tag: para
#: yast2_ncurses.xml:2352
#, no-c-format
msgid "Sets a value to a variable:"
msgstr "変数に値を設定します:"

#. Tag: screen
#: yast2_ncurses.xml:2355
#, no-c-format
msgid ""
"\n"
"&prompt.sudo;yast sysconfig set DISPLAYMANAGER=gdm\n"
msgstr ""
"\n"
"&prompt.sudo;yast sysconfig set DISPLAYMANAGER=gdm\n"

#. Tag: title
#: yast2_ncurses.xml:2359
#, no-c-format
msgid "Variable in Multiple Files"
msgstr "複数のファイル内にある変数の場合"

