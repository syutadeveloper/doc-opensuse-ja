#: yast2_ncurses.xml:1102
#, no-c-format
msgid ""
"\n"
"&prompt.sudo;yast ftp-server umask set_umask=177:077\n"
msgstr ""
"\n"
"&prompt.sudo;yast ftp-server umask set_umask=177:077\n"

#. Tag: term
#: yast2_ncurses.xml:1108
#, no-c-format
msgid "welcome_message"
msgstr "welcome_message"

#. Tag: para
#: yast2_ncurses.xml:1110
#, no-c-format
