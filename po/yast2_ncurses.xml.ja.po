msgstr ""
"\n"
"&prompt.sudo;yast kdump smtppass pass=/path/to/file\n"

#. Tag: term
#: yast2_ncurses.xml:1396
#, no-c-format
msgid "smtpserver"
msgstr "smtpserver"

#. Tag: para
#: yast2_ncurses.xml:1398
#, no-c-format
msgid "Specifies the SMTP server host name used for sending notification messages:"
msgstr "通知メッセージを送信する際に使用する SMTP サーバを設定します:"

#. Tag: screen
#: yast2_ncurses.xml:1402
#, no-c-format
msgid ""
"\n"
"&prompt.sudo;yast kdump smtpserver server=smtp.server.com\n"
msgstr ""
"\n"
"&prompt.sudo;yast kdump smtpserver server=smtp.server.com\n"

#. Tag: term
#: yast2_ncurses.xml:1408
#, no-c-format
msgid "smtpuser"
msgstr "smtpuser"

