#: yast2_ncurses.xml:1313
#, no-c-format
msgid ""
"\n"
"&prompt.sudo;yast kdump keepolddumps no=5\n"
msgstr ""
"\n"
"&prompt.sudo;yast kdump keepolddumps no=5\n"

#. Tag: term
#: yast2_ncurses.xml:1319
#, no-c-format
msgid "kernelcommandline"
msgstr "kernelcommandline"

#. Tag: para
#: yast2_ncurses.xml:1321
#, no-c-format
msgid "Specifies the command line that needs to be passed off to the kdump kernel:"
msgstr "kdump カーネルに渡す必要のあるコマンドラインを設定します:"

#. Tag: screen
#: yast2_ncurses.xml:1325
#, no-c-format
msgid ""
"\n"
"&prompt.sudo;yast kdump kernelcommandline command=\"ro root=LABEL=/\"\n"
msgstr ""
"\n"
"&prompt.sudo;yast kdump kernelcommandline command=\"ro root=LABEL=/\"\n"

#. Tag: term
#: yast2_ncurses.xml:1331
#, no-c-format
msgid "kernelcommandlineappend"
msgstr "kernelcommandlineappend"

#. Tag: para
#: yast2_ncurses.xml:1333
#, no-c-format
