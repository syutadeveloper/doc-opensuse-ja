#: help_admin.xml:213
#, no-c-format
msgid "<filename>MANIFEST</filename>"
msgstr "<filename>MANIFEST</filename>"

#. Tag: para
#: help_admin.xml:216
#, no-c-format
msgid "List of files with a brief summary."
msgstr "簡潔な説明を含むファイルの一覧です。"

#. Tag: term
#: help_admin.xml:222
#, no-c-format
msgid "<filename>NEWS</filename>"
msgstr "<filename>NEWS</filename>"

#. Tag: para
#: help_admin.xml:225
#, no-c-format
