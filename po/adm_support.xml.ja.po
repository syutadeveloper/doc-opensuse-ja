#: adm_support.xml:1842
#, no-c-format
msgid "$STATUS_CRITICAL"
msgstr ""

#. Tag: para
#: adm_support.xml:1844
#, no-c-format
msgid "Send only SCA reports that include a CRITICAL."
msgstr ""

#. Tag: term
#: adm_support.xml:1850
#, no-c-format
msgid "$STATUS_WARNING"
msgstr ""

#. Tag: para
#: adm_support.xml:1852
#, no-c-format
msgid "Send only SCA reports that include a WARNING or CRITICAL."
msgstr ""

#. Tag: term
#: adm_support.xml:1858
#, no-c-format
msgid "$STATUS_RECOMMEND"
msgstr ""

#. Tag: para
#: adm_support.xml:1860
#, no-c-format
msgid "Send only SCA reports that include a RECOMMEND, WARNING or CRITICAL."
msgstr ""

#. Tag: term
#: adm_support.xml:1866
